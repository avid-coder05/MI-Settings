<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:settings="http://schemas.android.com/apk/res/com.android.settings" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:miuisettings="http://schemas.android.com/apk/miuisettings" android:title="@string/freeform_guide_settings">
    <PreferenceCategory>
        <com.android.settingslib.miuisettings.preference.Preference
            android:enabled="false"
            android:layout="@layout/freeform_vedio"/>
    </PreferenceCategory>
    <PreferenceCategory android:key="freeform_dock_assistant">
        <com.android.settingslib.miuisettings.preference.Preference
            android:title="@string/gd_setting_title"
            android:key="dock_assistant"
            settings:controller="com.android.settings.special.SuperAssistantController">
            <intent
                android:targetPackage="com.miui.securitycenter"
                android:action="com.miui.dock.ACTION_SETTINGS"/>
        </com.android.settingslib.miuisettings.preference.Preference>
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/operation_guide_title"
        android:key="freeform_guide_category">
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/multi_window_cvw"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/multi_window_cvw_title"
            android:key="key_multi_window_cvw"
            android:summary="@string/multi_window_cvw_summary"
            miuisettings:showIcon="true"/>
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/multi_window_cvw"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/multi_window_cvw_title"
            android:key="key_multi_window_cvw_global"
            android:summary="@string/multi_window_cvw_summary_global"
            miuisettings:showIcon="true"/>
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/freeform_guide_notification_drop_down"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/freeform_guide_notification_drop_down_title"
            android:key="key_freeform_guide_notification_drop_down"
            android:summary="@string/freeform_guide_notification_drop_down_summary"
            miuisettings:showIcon="true"/>
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/freeform_guide_move"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/freeform_guide_move_title"
            android:key="key_freeform_guide_move"
            android:summary="@string/freeform_guide_move_summary"
            miuisettings:showIcon="true"/>
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/freeform_guide_to_sidehide"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/freeform_guide_to_sidehide_title"
            android:key="key_freeform_guide_to_sidehide"
            android:summary="@string/freeform_guide_to_sidehide_summary"
            miuisettings:showIcon="true"/>
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/freeform_guide_drop_down_to_fullscreen"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/freeform_guide_drop_down_to_fullscreen_title"
            android:key="key_freeform_guide_drop_down_to_fullscreen"
            android:summary="@string/freeform_guide_drop_down_to_fullscreen_summary"
            miuisettings:showIcon="true"/>
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/freeform_guide_move_to_small_freeform_window"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/freeform_guide_move_to_small_freeform_window_title"
            android:key="key_freeform_guide_move_to_small_freeform_window"
            android:summary="@string/freeform_guide_move_to_small_freeform_window_summary"
            miuisettings:showIcon="true"/>
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/freeform_guide_slide_to_small_freeform"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/freeform_guide_slide_to_small_freeform_title"
            android:key="key_freeform_guide_slide_to_small_freeform"
            android:summary="@string/freeform_guide_slide_to_small_freeform_summary"
            miuisettings:showIcon="true"/>
        <com.android.settingslib.miuisettings.preference.Preference
            android:icon="@drawable/freeform_guide_slide_to_small_freeform_global"
            android:layout="@layout/freeform_guide_item"
            android:title="@string/freeform_guide_slide_to_small_freeform_title"
            android:key="key_freeform_guide_slide_to_small_freeform_global"
            android:summary="@string/freeform_guide_slide_to_small_freeform_summary"
            miuisettings:showIcon="true"/>
    </PreferenceCategory>
</PreferenceScreen>
